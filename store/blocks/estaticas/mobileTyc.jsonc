    "disclosure-layout#politica-datos": {
        "children": ["disclosure-trigger#poltica-datos", "disclosure-content#poltica-datos"]
    },
    "disclosure-trigger#poltica-datos": {
        "children": ["rich-text#politica-datos"],
        "props":{

        "props":{
          "blockClass":"contenedor"
        }
    },
    // imagen
    "image#institucionalM": {
        "props": {
            "blockClass":["bannerEstaticas"],
            "src": "https://batadev--batanew.myvtex.com/arquivos/estaticasNewMobileBanners.png"
        }
    }, 
    "rich-text#insitucional": {
        "props": {
          "text": "## Institucional",
          "blockClass":["Contacto","institucionalTitulo"]
        }
    },

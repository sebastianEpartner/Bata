{
    "store.custom#institucional": {
        "children":[
            "image#institucionalM",
            "rich-text#insitucional",
            "disclosure-layout#politica-datos",
            "disclosure-layout#nuestro-calzado",
            "disclosure-layout#todo-pies",
            "disclosure-layout#empleado",
            "disclosure-layout#nuestro-compromiso",
            "disclosure-layout#tyc",
            "disclosure-layout#tiendas",
            "disclosure-layout#contactenos"

    },
    "rich-text#cards-2": {
        "props": {
          "text": "Cuidado de la piel",
          "blockClass": "titulosCards"
        }
    },
    "rich-text#cards-content-2": {
        "props": {
          "text": "Manejamos toda la linea de cuidado de tu piel al mejor precio.",
          "blockClass": "contenidoInfocards"
        }
      },
    // card 3
    "flex-layout.col#cards-3": {
        "children": [
          "image#cards-3",
          "rich-text#cards-3",
          "rich-text#cards-content-3"
        ]
    },
    "image#cards-3": {
        "props": {
          "src": "https://cutis.vteximg.com.br/arquivos/cards-3.png",
          "maxHeight": "",
          "maxWidth": "",
          "blockClass": "imagenCard"
        }
    },
    "rich-text#cards-3": {
        "props": {
          "text": "Medicamentos",
